<div class="{{ overrideClassSearch }} su-search-content">
	<form class="su-search-content_form" action="#" method="post">
		<div class="su-search-content_line su-field-search">
			<input class="su-field-search_input" type="search" name="#" placeholder="Найти" value="">
			<span class="su-field-search_postfix"></span>
		</div>
		<div class="su-search-popup">
			<ul class="su-search-popup_list su-list">

				{% for row in searchPopup %}
					<li class="su-list_item">
						<div class="su-search-popup_name">{{ row.name }}</div>
						<div class="su-search-popup_quantity">{{ row.quantity }}</div>
					</li>
				{% endfor %}

			</ul>
		</div>
	</form>
</div>