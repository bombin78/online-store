<section class="su-sidebar-bnr">

	<ul class="su-sidebar-bnr_list su-list">

		{% for row in sidebarBanner %}
			<li class="su-list_item">
				<div class="su-list_bnr su-bnr-block">
					<a class="su-bnr-block_link su-link-img {{ row.modifier }}" href="{{ row.href }}">
						<img class="su-link-img_img" src="{{ row.src }}" alt="{{ row.alt }}">
					</a>
				</div>
			</li>
		{% endfor %}

	</ul>
</section>
